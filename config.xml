<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.phonegap.amphitrite" version="1.0.0">
  <name>Les Monstres d'Amphitrite</name>
  <description>Bande dessinée numérique. Application, version exposition.</description>
  <author href="http://www.anthonyrageul.net">Tony</author>
  <content src="index.html"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="14"/>
    <platform name="android">
    <icon src="www/images/icon/android/drawable-ldpi-icon.png" density="ldpi"/>
    <icon src="www/images/icon/android/drawable-mdpi-icon.png" density="mdpi"/>
    <icon src="www/images/icon/android/drawable-hdpi-icon.png" density="hdpi"/>
    <icon src="www/images/icon/android/drawable-xhdpi-icon.png" density="xhdpi"/>
    <icon src="www/images/icon/android/drawable-xxhdpi-icon.png" density="xxhdpi"/>
    <icon src="www/images/icon/android/drawable-xxxhdpi-icon.png" density="xxxhdpi"/>
  </platform>
  <platform name="ios">
    <icon src="www/images/icon/iOS/icon.png" platform="ios" width="57" height="57"/>
    <icon src="www/images/icon/iOS/icon@2x.png" platform="ios" width="114" height="114"/>
    <icon src="www/images/icon/iOS/icon-40.png" platform="ios" width="40" height="40"/>
    <icon src="www/images/icon/iOS/icon-40@2x.png" platform="ios" width="80" height="80"/>
    <icon src="www/images/icon/iOS/icon-50.png" platform="ios" width="50" height="50"/>
    <icon src="www/images/icon/iOS/icon-50@2x.png" platform="ios" width="100" height="100"/>
    <icon src="www/images/icon/iOS/icon-60.png" platform="ios" width="60" height="60"/>
    <icon src="www/images/icon/iOS/icon-60@2x.png" platform="ios" width="120" height="120"/>
    <icon src="www/images/icon/iOS/icon-60@3x.png" platform="ios" width="180" height="180"/>
    <icon src="www/images/icon/iOS/icon-72.png" platform="ios" width="72" height="72"/>
    <icon src="www/images/icon/iOS/icon-72@2x.png" platform="ios" width="144" height="144"/>
    <icon src="www/images/icon/iOS/icon-76.png" platform="ios" width="76" height="76"/>
    <icon src="www/images/icon/iOS/icon-76@2x.png" platform="ios" width="152" height="152"/>
    <icon src="www/images/icon/iOS/icon-small.png" platform="ios" width="29" height="29"/>
    <icon src="www/images/icon/iOS/icon-small@2x.png" platform="ios" width="58" height="58"/>
    <icon src="www/images/icon/iOS/icon-small@3x.png" platform="ios" width="87" height="87"/>
